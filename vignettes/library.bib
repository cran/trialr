doi = {10.1191/1740774506cn134oa},
file = {:C$\backslash$:/Users/krist/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Garrett-Mayer - 2006 - The continual reassessment method for dose-finding studies a tutorial.pdf:pdf},
