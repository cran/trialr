isbn = {1527-7755 (Electronic) 0732-183X (Linking)},
issn = {0732183X},
journal = {Journal of Clinical Oncology},
number = {31},
pages = {4982--4986},
pmid = {17971597},
title = {{Translation of innovative designs into phase I trials}},
volume = {25},
year = {2007}
}
