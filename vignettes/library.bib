file = {:C$\backslash$:/Users/krist/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Arlt et al. - 1999 - Biotransformation of oral dehydroepiandrosterone in elderly men Significant increase in circulating estrogens.pdf:pdf},
isbn = {0021-972X},
