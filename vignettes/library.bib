journal = {Journal of the National Cancer Institute},
keywords = {Antineoplastic Agents,Antineoplastic Agents: administration {\&} dosage,Antineoplastic Agents: adverse effects,Clinical Trials, Phase I as Topic,Clinical Trials, Phase I as Topic: methods,Dose-Response Relationship, Drug,Humans,Maximum Tolerated Dose,Research Design},
number = {3},
