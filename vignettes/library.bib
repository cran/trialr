journal = {Biometrics},
keywords = {CRM,DoseFinding,TITE-CRM,continual reassessment method,dose limiting,late-onset toxicities,likelihood-based design,phase i trial,time-to-event},
mendeley-tags = {CRM,DoseFinding,TITE-CRM},
number = {4},
pages = {1177--1182},
pmid = {11129476},
title = {{Sequential designs for phase I clinical trials with late-onset toxicities.}},
