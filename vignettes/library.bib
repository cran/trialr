isbn = {10.1186/1745-6215-8-16},
issn = {1745-6215},
journal = {Trials},
keywords = {Biomedicine general,Health Sciences,Medicine,Medicine/Public Health,MetaAnalysis,README,Statistics for Life Sciences,general},
mendeley-tags = {MetaAnalysis,README},
number = {1},
