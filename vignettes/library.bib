file = {:C$\backslash$:/Users/krist/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Freidlin, McShane, Korn - 2010 - Randomized clinical trials with biomarkers Design issues.pdf:pdf},
