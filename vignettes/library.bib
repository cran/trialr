isbn = {1533-4406 (Electronic) 0028-4793 (Linking)},
issn = {1533-4406},
journal = {The New England journal of medicine},
keywords = {NSCLC,PD1,Pembrolizumab},
mendeley-tags = {NSCLC,PD1,Pembrolizumab},
number = {21},
pages = {2018--28},
pmid = {25891174},
title = {{Pembrolizumab for the treatment of non-small-cell lung cancer.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25891174},
volume = {372},
