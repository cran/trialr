doi = {10.1016/0895-4356(94)90086-8},
file = {:C$\backslash$:/Users/krist/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Salen, Erik, Nordemar - 1994 - The disability rating index An instrument for the assessment of disability in clinical settings.pdf:pdf},
isbn = {0895-4356},
issn = {08954356},
journal = {Journal of Clinical Epidemiology},
keywords = {DRI,Disability evaluation,Evaluation study,Outcome assessment,Physical disability},
