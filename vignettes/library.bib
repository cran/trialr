journal = {Statistics in Medicine},
keywords = {Continual reassessment method,Dose-finding,DoseFinding,Efficacy,EfficacyToxicity,Model-based designs,Toxicity,Trinary outcome},
mendeley-tags = {DoseFinding,EfficacyToxicity},
number = {10},
pages = {1077--1083},
pmid = {20419760},
title = {{Model-based phase I designs incorporating toxicity and efficacy for single and dual agent drug combinations: Methods and challenges}},
