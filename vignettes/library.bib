file = {:C$\backslash$:/Users/krist/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fidler et al. - 2016 - Degrees of Freedom in Planning, Running, Analyzing, and Reporting Psychological Studies A Checklist to Avoid p-Ha.pdf:pdf},
title = {{Degrees of Freedom in Planning, Running, Analyzing, and Reporting Psychological Studies: A Checklist to Avoid p-Hacking}},
